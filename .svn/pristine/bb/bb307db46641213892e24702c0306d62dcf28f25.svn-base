@font-face {
    font-family: "BryantRegular";
    src: url('https://wimcloud.usgs.gov/fonts/Bryant2Combo/Bryant2/Bryant-Regular.otf'), url('https://wimcloud.usgs.gov/fonts/Bryant2Combo/Bryant2Web_EOT/BryantWebRegular.eot');
}
@font-face {
    font-family: "BryantBold";
    src: url('https://wimcloud.usgs.gov/fonts/Bryant2Combo/Bryant2/Bryant-Bold.otf'), url('https://wimcloud.usgs.gov/fonts/Bryant2Combo/Bryant2Web_EOT/BryantWebBold.eot');
}

.loginCenter 
{
    position:relative;
    top: 15%;    
}

#loginForm 
{
   border: 1px solid #59595c;
   color:#E7E4CE;
   background: #59595C;
   filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#7E7E84', endColorstr='#59595C',GradientType=0 ); /* IE6-9 */
   background: -webkit-gradient(linear, left top, left bottom, from(#7E7E84), to(#59595C));
   background: -webkit-linear-gradient(top, #7E7E84, #59595C);
   background: -moz-linear-gradient(top, #7E7E84, #59595C);
   background: -ms-linear-gradient(top, #7E7E84, #59595C);
   background: -o-linear-gradient(top, #7E7E84, #59595C);
   background-image: -ms-linear-gradient(top, #7E7E84 0%, #59595C 100%);
   -webkit-border-radius: 8px;
   -moz-border-radius: 8px;
   border-radius: 8px;
   -webkit-box-shadow: rgba(000,000,000,0.9) 0 1px 2px, inset rgba(255,255,255,0.4) 0 0px 0;
   -moz-box-shadow: rgba(000,000,000,0.9) 0 1px 2px, inset rgba(255,255,255,0.4) 0 0px 0;
   box-shadow: rgba(000,000,000,0.9) 0 1px 2px, inset rgba(255,255,255,0.4) 0 0px 0;
   /*font-family: 'Helvetica Neue',Helvetica,sans-serif;*/
   font-family: 'BryantRegular';
   text-decoration: none;
   vertical-align: middle;
   min-width:300px;
   padding:20px;
   width:300px; 
   margin: auto;   
}
#loginForm h2 {
   text-shadow: #a19f90 0 1px 0;
   font-size:24px;
   margin: 0 0 20px 0;
   font-family: 'BryantBold';
   text-align:center;
   color:#E7E4CE;
}

.login-title {
   margin-bottom:10px;
   color: #E7E4CE;
   text-shadow: #a19f90 0 1px 0;
}

.login-field {
   border: 1px solid #000000;
   background: #ffffff;
   -webkit-border-radius: 4px;
   -moz-border-radius: 4px;
   border-radius: 4px;
   color: #2b3a47;
   -webkit-box-shadow: rgba(255,255,255,0.4) 0 1px 0, inset rgba(000,000,000,0.7) 0 0px 0px;
   -moz-box-shadow: rgba(255,255,255,0.4) 0 1px 0, inset rgba(000,000,000,0.7) 0 0px 0px;
   box-shadow: rgba(255,255,255,0.4) 0 1px 0, inset rgba(000,000,000,0.7) 0 0px 0px;
   padding:8px;
   margin-bottom:20px;
   width:280px;
}
.login-field:focus {
   background: #fff;
}

a.LaMPButton, input.LaMPButton 
{
    -webkit-border-radius: 3px; 
    -moz-border-radius: 3px;
    border-radius: 3px;
    /*font-family:arial, helvetica, sans-serif; */
    font-family: 'BryantBold';
    padding: 6px; 
    /*text-shadow: -1px -1px 0 rgba(0,0,0,0.3);*/
    text-align: center; 
    margin-bottom:1px;
}

a.LaMPButton.green   
{
    border:1px solid #f37a49; 
    color: #FFFFFF; 
    background-color: #f37a49;
   background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #f6a17f), color-stop(100%, #f37a49));
    background-image: -webkit-linear-gradient(top, #f6a17f, #f37a49);
    background-image: -moz-linear-gradient(top, #f6a17f, #f37a49);
    background-image: -ms-linear-gradient(top, #f6a17f, #f37a49);
    background-image: -o-linear-gradient(top, #f6a17f, #f37a49);
    background-image: linear-gradient(top, #f6a17f, #f37a49);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#f6a17f, endColorstr=#f37a49, GradientType=0);
}
a.LaMPButton.green:hover{
  border:1px solid #f37a49; background-color: #f6a17f;
    background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f6a17f), color-stop(100%, #f37a49));
    background-image: -webkit-linear-gradient(top, #f6a17f, #f37a49);
    background-image: -moz-linear-gradient(top, #f6a17f, #f37a49);
    background-image: -ms-linear-gradient(top, #f6a17f, #f37a49);
    background-image: -o-linear-gradient(top, #f6a17f, #f37a49);
    background-image: linear-gradient(top, #f6a17f, #f37a49);
    filter:progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr=#f6a17f, endColorstr=#f37a49);
}
a.LaMPButton.blue, input.LaMPButton.blue
{
    padding:5px;
    border:1px solid #1e2933; 
    color: #E7E4CE; 
    background-color: #59595C;
    font-size: medium;
    background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #7E7E84), color-stop(100%, #59595C));
    background-image: -webkit-linear-gradient(top, #7E7E84, #59595C);
    background-image: -moz-linear-gradient(top, #7E7E84, #59595C);
    background-image: -ms-linear-gradient(top, #7E7E84, #59595C);
    background-image: -o-linear-gradient(top, #7E7E84, #59595C);
    background-image: linear-gradient(top, #7E7E84, #59595C);
    filter:progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr=#7E7E84, endColorstr=#59595C);
}
a.LaMPButton.blue:hover, input.LaMPButton.blue:hover
{
    border:1px solid #1e2933; 
    background-color: #cccccc;
     color: #59595C;
     background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#cccccc), color-stop(100%, #cccccc));
     background-image: -webkit-linear-gradient(top, #cccccc, #cccccc);
     background-image: -moz-linear-gradient(top, #cccccc, #cccccc);
     background-image: -ms-linear-gradient(top, #cccccc, #cccccc);
     background-image: -o-linear-gradient(top, #cccccc, #cccccc);
     background-image: linear-gradient(top, #cccccc, #cccccc);
    filter:progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr=#cccccc, endColorstr=#cccccc);
}
a.LaMPButton.icon 
{
    padding-left: 11px;    
    height:20px;
    display: inline-block;
    font-size: medium;
}

a.LaMPButton:active
{
    margin-top:1px;
    margin-bottom:0px;
}


a.LaMPButton.icon span
{  
    height: 23px;
    display:table-cell;
    vertical-align: middle;
}

a.LaMPButton.icon span.editIcon
{  
    padding-left: 30px;
    /*background: url(../Images/edit.png) no-repeat 0 0px; */
    background-image:url(../Images/edit.png);
    background-repeat:no-repeat;
    
}

a.LaMPButton.icon span.loginIcon
{  
    padding-left: 26px;
    /*background: url(../Images/lock.png) no-repeat 0 0px; */
    background-image: url(../Images/lock.png);
    background-repeat: no-repeat;
}

