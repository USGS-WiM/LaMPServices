	background: -webkit-linear-gradient(left,  #E5E5E5 0%,#F2F2F2 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(left,  #E5E5E5 0%,#F2F2F2 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(left,  #E5E5E5 0%,#F2F2F2 100%); /* IE10+ */
	background: linear-gradient(to right,  #E5E5E5 0%,#F2F2F2 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#E5E5E5', endColorstr='#F2F2F2',GradientType=1 ); /* IE6-9 */
}

