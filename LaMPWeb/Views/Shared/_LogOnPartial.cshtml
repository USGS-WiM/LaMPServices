@if(Request.IsAuthenticated) {
    <text>Welcome <strong>@Html.ActionLink(User.Identity.Name, "DataManagerDetailsByName", "DataManager", new { name = User.Identity.Name }, new { @onclick = "loading();" }) </strong>!
    [ @Html.ActionLink("Log Off", "LogOff", "Account", null, new { @style = "font-family:'BryantBold'" }) ]</text>
}
else {
    @:[ @Html.ActionLink("Log On", "LogOn", "Account") ]
}
