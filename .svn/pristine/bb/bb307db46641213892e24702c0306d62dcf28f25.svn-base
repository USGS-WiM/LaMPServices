	background: -o-linear-gradient(left,  #59595C 0%,#ffffff 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(left,  #59595C 0%,#ffffff 100%); /* IE10+ */
	background: linear-gradient(to right,  #59595C 0%,#ffffff 100%); /* W3C */
